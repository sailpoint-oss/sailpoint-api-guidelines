{
  "name": "sailpoint-api-guidelines",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "types:check": "fumadocs-mdx && next typegen && tsc --noEmit",
    "postinstall": "fumadocs-mdx",
    "lint": "biome check",
    "format": "biome format --write",
    "rules:generate": "bun run ./scripts/generate-rules-json.ts",
    "reviews:stubs": "bun run ./scripts/generate-rule-review-stubs.ts",
    "reviews:validate": "bun run ./scripts/validate-reviews.ts"
  },
  "dependencies": {
    "@base-ui/react": "1.0.0",
    "clsx": "^2.1.1",
    "fumadocs-core": "16.2.5",
    "fumadocs-mdx": "14.1.1",
    "fumadocs-ui": "^16.2.5",
    "lucide-react": "^0.562.0",
    "next": "16.0.10",
    "puppeteer": "^24.33.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/mdx": "^2.0.13",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  }
}
